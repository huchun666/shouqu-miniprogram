/* pages/community/community.wxss */
.community-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 32rpx;
}

.title {
  font-size: 40rpx;
  font-weight: 600;
  color: var(--text-primary);
}

.create-btn {
  padding: 12rpx 24rpx;
  background: var(--primary-color);
  color: #fff;
  border-radius: 20rpx;
  font-size: 28rpx;
  border: none;
}
